{
    "name": "kodus/session",
    "description": "A simple interface for storing and retrieving session data without the use of PHP's native session handling",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Nordahl Pedersen",
            "email": "thno@fynskemedier.dk"
        },
        {
            "name": "Rasmus Schultz",
            "email": "rasc@jfmedier.dk"
        },
	{
	    "name": "Bo Andersen",
	    "email": "boan@jfmedier.dk"
	}
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "psr/http-message": "^1",
        "psr/http-server-handler": "^1",
        "psr/http-server-middleware": "^1",
        "psr/simple-cache": "^1",
        "middlewares/client-ip": "^2",
        "kodus/uuid-v4": "dev-upgrade-php-81 as 1.1.0",
        "kodus/uuid-v5": "^1"
    },
    "require-dev": {
        "codeception/codeception": "^2",
        "zendframework/zend-diactoros": "^1.3",
        "kodus/mock-cache": "^1"
    },
    "autoload": {
        "psr-4": {
            "Kodus\\Session\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kodus\\Session\\Tests\\Unit\\" : "tests/unit"
        }
    }
}
